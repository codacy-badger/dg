{
  "name": "dylan-gattey",
  "version": "0.0.1",
  "scripts": {
    "prestart": "tsc",
    "postinstall": "npm run typings install",
    "tsc": "tsc",
    "typings": "typings",
    "build-js": "tsc -w",
    "build-css": "node-sass -w app/styles/ -o app/styles/",
    "serve": "lite-server",
    "compile": "concurrent \"npm run build-js\" \"npm run build-css\" ",
    "start": "concurrent \"npm run build-js\" \"npm run build-css\" \"npm run serve\" "
  },
  "dependencies": {
    "angular2": "2.0.0-beta.7",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.33.3",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "systemjs": "0.19.20",
    "zone.js": "0.5.15"
  },
  "devDependencies": {
    "concurrently": "^1.0.0",
    "lite-server": "^2.1.0",
    "node-normalize-scss": "^1.3.0",
    "node-sass": "^3.4.2",
    "susy": "^2.2.12",
    "typescript": "^1.8.2",
    "typings": "^0.6.8"
  }
}
